{
  "version": 2,
  "builds": [{ "src": "frontend/**", "use": "@vercel/static" }],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|webp|svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true
}
